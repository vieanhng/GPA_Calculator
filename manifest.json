{
  "name": "GPA Calculator for TMUer",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Extension help calculator gpa faster. Author: vieanhng. Copyright (C) 2023 vieanhng.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://congdaotao.tmu.edu.vn/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://congdaotao.tmu.edu.vn/*"],
      "js": ["js/contentScript.js"],
      "css": ["style/style.css"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup/index.html",
    "default_title": "GPA Calculator"
  },
  "icons":{
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "nva4work@gmail.com"
}
